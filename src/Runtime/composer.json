{
    "name": "kraken-php/runtime",
    "description": "Kraken Framework Runtime Component.",
    "keywords": [
        "kraken", "kraken-php", "thread", "process", "runtime", "abstraction", "agent", "service"
    ],
    "license": "MIT",
    "homepage": "http://kraken-php.com",
    "support": {
        "issues": "https://github.com/kraken-php/framework/issues",
        "source": "https://github.com/kraken-php/framework"
    },
    "authors": [{
        "name": "Kamil Jamroz"
    }],
    "require": {
        "php": ">=5.5.9",
        "kraken-php/channel": "0.4.*",
        "kraken-php/core": "0.4.*",
        "kraken-php/event": "0.4.*",
        "kraken-php/filesystem": "0.4.*",
        "kraken-php/log": "0.4.*",
        "kraken-php/loop": "0.4.*",
        "kraken-php/promise": "0.4.*",
        "kraken-php/supervision": "0.4.*",
        "kraken-php/throwable": "0.4.*",
        "kraken-php/util": "0.4.*"
    },
    "autoload": {
        "psr-4": {
            "Kraken\\Runtime\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
